	keywords = {4215 Climate and interannual variability, 4262 Ocean observing systems, 4512 Currents, 4556 Sea level: variations and mean, 4576 Western boundary currents, ADCP, Gulf Stream transport, interannual variability, long-term trend, repeat measurement},
	pages = {2013GL058636},
	file = {Snapshot:/Users/sgaichas/Library/Application Support/Firefox/Profiles/oa3vrvq8.default-1484688149066/zotero/storage/UPRDJDJ3/abstract.html:text/html}
}

