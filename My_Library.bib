	keywords = {Arrowtooth flounder, Gulf of Alaska, multispecies, Pacific cod, Pacific halibut, predation, Steller sea lion, stock assessment, Walleye Pollock},
	pages = {184--205},
	file = {Snapshot:/Users/sgaichas/Library/Application Support/Firefox/Profiles/oa3vrvq8.default-1484688149066/zotero/storage/9A368ZNM/abstract.html:text/html}
